---
import { storyblokEditable } from "@storyblok/astro";
const {blok} = Astro.props

console.log(blok.image)
---

<div id="work" class="flex flex-col w-full rounded-3xl scroll-mt-24 aspect-square" {...storyblokEditable(blok)} >
    <div class="flex flex-col w-full bg-cover rounded-3xl h-full items-center justify-center overflow-hidden" style=`background-image:url('${blok.image.filename}')` ></div>
</div>

